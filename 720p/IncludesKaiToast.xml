<includes>
	<include name="KaiToast">
		<control type="group">
                        <animation effect="slide" start="0,0" end="0,-38" time="0" condition="Window.IsVisible(Home)">conditional</animation>
                        <animation effect="slide" start="0,0" end="0,30" time="0" condition="!Window.IsVisible(Home) + !Window.IsVisible(2005) + !Window.IsVisible(2006)">conditional</animation>
			<posx>0</posx>
			<posy>655</posy>
                        <width>1280</width>
                        <height>35</height>
                        <itemgap>0</itemgap>
                        <orientation>horizontal</orientation>
                        <control type="image">
                                <posx>-30</posx>
                                <posy>0</posy>
                                <width>1340</width>
                                <height>35</height>
                                <colordiffuse>EEFFFFFF</colordiffuse>
                                <texture>OverlayDialogBackground.png</texture>
<!--                                <texture border="5" flipy="true">HomeSubNF.png</texture>-->
		                <colordiffuse>99FFFFFF</colordiffuse>
				<visible>Window.IsVisible(Home)</visible>
			</control>
			<control type="fadelabel" id="401">
				<description>Line 1 Label</description>
				<posx>20</posx>
				<posy>0</posy>
				<width>480</width>
				<height>35</height>
				<font>font13_title</font>
				<textcolor>grey</textcolor>
				<align>left</align>
				<aligny>center</aligny>
				<scrollout>false</scrollout>
				<pauseatend>2000</pauseatend>
                <animation effect="slide" start="0,0" end="60,0" time="0" condition="!Window.IsVisible(2005) + !Window.IsVisible(2006) + !Window.IsVisible(Home)">conditional</animation>
			</control>
			<control type="fadelabel" id="402">
				<description>Line 2 Label</description>
				<posx>580r</posx>
				<posy>0</posy>
				<width>560</width>
				<height>35</height>
				<font>font12</font>
				<textcolor>grey2</textcolor>
				<align>right</align>
				<aligny>center</aligny>
				<scrollout>false</scrollout>
				<pauseatend>2000</pauseatend>
	                        <animation effect="slide" start="0,0" end="-135,0" time="0" condition="Window.IsVisible(2006)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-160,0" time="0" condition="Window.IsVisible(VideoOSD)">conditional</animation>							
	                        <animation effect="slide" start="0,0" end="-180,0" time="0" condition="Window.IsVisible(2005) + [Player.ShowInfo + !Window.IsVisible(VideoOSD)]">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-35,0" time="0" condition="Window.IsVisible(2005) + VideoPlayer.Content(LiveTV) + Player.Recording + !Window.IsActive(VideoOSD)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-70,0" time="0" condition="Window.IsVisible(2005) + !IsEmpty(VideoPlayer.AudioLanguage) + !Window.IsActive(VideoOSD)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-70,0" time="0" condition="Window.IsVisible(2005) + !IsEmpty(VideoPlayer.SubtitlesLanguage) + !Window.IsActive(VideoOSD)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-35,0" time="0" condition="VideoPlayer.IsStereoscopic + Window.IsActive(VideoOSD)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-60,0" time="0" condition="!Window.IsVisible(2005) + !Window.IsVisible(2006) + !Window.IsVisible(Home)">conditional</animation>
			</control>
		</control>
	</include>
	<include name="KaiToastTouch">
		<control type="group">
			<posx>0</posx>
			<posy>655</posy>
			<width>1280</width>
			<height>35</height>
			<itemgap>0</itemgap>
			<orientation>horizontal</orientation>
			<control type="fadelabel" id="401">
				<description>Line 1 Label</description>
				<posx>20</posx>
				<posy>0</posy>
				<width>480</width>
				<height>35</height>
				<font>font13_title</font>
				<textcolor>grey</textcolor>
				<align>left</align>
				<aligny>center</aligny>
				<scrollout>false</scrollout>
				<pauseatend>2000</pauseatend>
			</control>
			<control type="fadelabel" id="402">
				<description>Line 2 Label</description>
				<posx>580r</posx>
				<posy>0</posy>
				<width>560</width>
				<height>35</height>
				<font>font12</font>
				<textcolor>grey2</textcolor>
				<align>right</align>
				<aligny>center</aligny>
				<scrollout>false</scrollout>
				<pauseatend>2000</pauseatend>
	                        <animation effect="slide" start="0,0" end="-135,0" time="0" condition="Window.IsVisible(2006)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-180,0" time="0" condition="Window.IsVisible(2005)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-35,0" time="0" condition="Window.IsVisible(2005) + VideoPlayer.Content(LiveTV) + Player.Recording">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-70,0" time="0" condition="Window.IsVisible(2005) + !IsEmpty(VideoPlayer.AudioLanguage)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-70,0" time="0" condition="Window.IsVisible(2005) + !IsEmpty(VideoPlayer.SubtitlesLanguage)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-35,0" time="0" condition="VideoPlayer.IsStereoscopic">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-150,0" time="0" condition="!Window.IsVisible(2005) + !Window.IsVisible(2006) + !Window.IsVisible(Home)">conditional</animation>
	                        <animation effect="slide" start="0,0" end="-210,0" time="0" condition="Window.IsVisible(Home)">conditional</animation>
			</control>
		</control>
	</include>
</includes>
        <control type="button">
            <posx>0</posx>
            <posy>0</posy>
            <width>ScreenWidth</width>
            <height>ScreenHeight</height>
            <texturefocus>-</texturefocus>
            <texturenofocus>-</texturenofocus>
            <onclick>ActivateWindow(MusicOSD)</onclick>
        </control>
